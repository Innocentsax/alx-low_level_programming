# 2-Makefile

# Setting variables
CC = gcc
SRC = main.c holberton.c
NAME = holberton
OBJ = $(SRC:.c=.o) # Substitution References

# Rule all: Build the holberton executable file
# 1. Implicity rule to updating .o files
# 2. Then compile with .o files and executable
all: $(OBJ)
	$(CC) $(OBJ) -o $(NAME)
